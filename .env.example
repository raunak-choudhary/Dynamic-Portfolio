# Supabase Configuration Template
# Copy this file to .env.local and fill in your actual values

# Supabase Project URL
REACT_APP_SUPABASE_URL=https://your-project-id.supabase.co

# Supabase Anonymous Key (public key)
REACT_APP_SUPABASE_ANON_KEY=your_anon_key_here

# Supabase Service Role Key (KEEP SECRET!)
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Database Configuration
DB_HOST=db.your-project-id.supabase.co
DB_NAME=postgres
DB_USER=postgres
DB_PORT=5432

# Environment
NODE_ENV=development

# Contact Configuration
REACT_APP_CONTACT_EMAIL=your_email@gmail.com

# Admin Credentials (temporary)
REACT_APP_ADMIN_USERNAME=admin
REACT_APP_ADMIN_PASSWORD=password

# Instructions:
# 1. Copy this file to .env.local
# 2. Replace all placeholder values with your actual Supabase credentials
# 3. Get your credentials from: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api
# 4. Never commit .env.local to version control


# Authentication Configuration
REACT_APP_JWT_SECRET=your-jwt-secret-key
REACT_APP_SESSION_DURATION=86400000
REACT_APP_AUTH_REFRESH_THRESHOLD=7200000